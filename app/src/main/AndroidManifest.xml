<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.learnjapanese">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HiraganaActivity" />
        <activity android:name=".HiraganaSlideActivity" />
        <activity android:name=".HiraganaTableActivity" />
        <activity android:name=".HiraganaListActivity" />
        <activity
            android:name=".HiraganaDrawerActivity"
            android:label="@string/title_activity_hiraga_drawer"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>